    "market": "company.name",
    "issuing_paper": "company.name",
    "pay_to": "person.fullName",
    "intermediary": "company.name",

    "policy_status_ennabl": "helpers.arrayElement:Active:Canceled:Expired:Bound:Quoted:Issued",
    "policy_status_ams": "helpers.arrayElement:Active:Canceled:Expired:Pending:Bound",
    "renewal_status": "helpers.arrayElement:Renewed:Non-Renew:Remarket:Pending:Lost",

    "product_segment": "helpers.arrayElement:SMB:Mid-Market:Enterprise",
