    "policy_number": "string.uuid",

    "revenue_billed": "number.float:500:250000:2",
    "premium_billed": "number.float:1000:500000:2",
    "commission_billed": "number.float:100:100000:2",

    "revenue_ennabl": "number.float:500:250000:2",
    "premium_ennabl": "number.float:1000:500000:2",
    "commission_ennabl": "number.float:100:100000:2",

    "revenue_ams": "number.float:500:250000:2",
    "premium_ams": "number.float:1000:500000:2",
    "commission_ams": "number.float:100:100000:2",

    "revenue_prod_credit": "number.float:0:250000:2",
    "premium_prod_credit": "number.float:0:500000:2",
    "production_credit": "number.float:0:100000:2",

    "effective_date": "date.between:2024-01-01:2026-12-31",
    "expiration_date": "date.afterColumn:effective_date:30:730",

    "am_best_rating": "A",
